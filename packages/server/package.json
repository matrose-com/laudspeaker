{
  "name": "laudspeaker-api",
  "version": "0.1.0",
  "description": "",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "env-cmd -f .env npm run prebuild && env-cmd -f .env nest start --watch",
    "start:dev:db": "./src/common/scripts/start-db.sh",
    "start:debug": "npm run prebuild && nest start --debug --watch",
    "start:prod": "env-cmd -f .env  node dist/src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "test": "env-cmd -f src/common/envs/development.env jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "env-cmd -f src/common/envs/development.env jest --config ./test/jest-e2e.json",
    "typeorm": "npm run build && typeorm-ts-node-commonjs -d ./src/data-source.ts",
    "typeorm:generate": "npm run typeorm migration:generate",
    "typeorm:create": "npx typeorm-ts-node-commonjs migration:create",
    "typeorm:run": "npm run typeorm migration:run",
    "typeorm:revert": "npm run typeorm migration:revert",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --release $(sentry-cli releases propose-version) ./dist && sentry-cli sourcemaps upload --release $(sentry-cli releases propose-version) ./dist"
  },
